{
  "name": "ap-truyendich",
  "version": "0.1.0",
  "private": true,
  "description": "PWA hỗ trợ điều dưỡng tính & theo dõi thời gian truyền dịch.",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --host --port 4173",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "e2e": "cross-env VITE_AUTH_MODE=mock start-server-and-test dev http://localhost:5173 \"playwright test\"",
    "e2e:preview": "cross-env VITE_AUTH_MODE=mock start-server-and-test preview http://localhost:4173 \"playwright test --project=chromium\""
  },
  "engines": { "node": ">=18.17.0" },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "lucide-react": "^0.454.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.47",
    "start-server-and-test": "^2.0.8",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.6.2",
    "vite": "^5.4.19",
    "vite-plugin-pwa": "^0.20.5",
    "vitest": "^2.0.5"
  }
}
